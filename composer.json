{
  "name": "soatok/anthrokit-auth",
  "description": "Authentication package for AnthroKit (Slim Framework 3)",
  "keywords": [
    "authentication",
    "password hashing",
    "twitter",
    "anthrokit",
    "furry",
    "furries",
    "furry fandom",
    "two-factor authentication",
    "multi-factor authentication",
    "Slim",
    "Slim Framework",
    "2FA",
    "MFA"
  ],
  "license": "ISC",
  "autoload": {
    "psr-4": {
      "Soatok\\AnthroKit\\Auth\\": "src/"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "abraham/twitteroauth": "^1|^2",
    "kelunik/two-factor": "^1",
    "soatok/anthrokit": "^0|^1",
    "laminas/laminas-mail": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "vimeo/psalm": "^3"
  },
  "scripts": {
    "test": "psalm"
  }
}